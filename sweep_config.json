{
  "method": "random",
  "metric": {
    "goal": "maximize",
    "name": "eval/mean_reward"
  },
  "parameters": {
    "do_shape": {
      "values": [true, false]
    },
  "learning_rate": {
    "distribution": "log_uniform_values",
    "min": 1e-5,
    "max": 5e-3
  },
    "buffer_size": {
      "distribution": "log_uniform_values",
      "min": 10000,
      "max": 500000
  },
  "batch_size": {
    "values": [32, 64, 128, 256, 512, 1024, 2048]
  },
  "learning_starts":{
    "distribution": "int_uniform",
    "min": 50,
    "max": 50000
  },
  "train_freq":{
    "values": [1,2,8,64,100,200,1000]
  },
  "tau":{
    "distribution": "log_uniform_values",
    "min": 1e-3,
    "max": 2e-1
  }
  }
}
